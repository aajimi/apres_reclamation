

{% block content %}
    <h1>Offre covoiturage creation</h1>

    {{ form_start(form) }}

    <div class="form-inline">{{ form_row(form.lieuDepart,{ 'attr' : {'class' : 'form-control'}}) }}</div>
    <div class="form-inline">{{ form_row(form.lieuArrivee,{ 'attr' : {'class' : 'form-control'}}) }}</div>
    <div class="form-inline">{{ form_row(form.dateDepart,{'attr' : {'class' : 'form-control'}}) }}</div>
    <div class="form-inline">{{ form_row(form.nbrPlacesDispo, {'attr' : {'class' : 'form-control'}}) }}</div>
    <div class="form-inline">{{ form_row(form.trajet, {'attr' : {'class' : 'form-control'}}) }}</div>
    <div class="form-inline">{{ form_row(form.flagAnnule,{'attr' : {'class' : 'form-control'}}) }}</div>
    <div class="form-inline">{{ form_row(form.flagTermine,{'attr' : {'class' : 'form-control'}}) }}</div>
    <div class="form-inline">{{ form_row(form.flagFumeur,{'attr' : {'class' : 'form-control'}}) }}</div>
    <div class="form-inline">{{ form_row(form.flagFemaleOnly,{'attr' : {'class' : 'form-control'}}) }}</div>
    <div class="form-inline">{{ form_row(form.typeCovoiturage,{ 'attr' : {'class' : 'form-control'}}) }}</div>
    <div class="form-inline">{{ form_row(form.test,{ 'attr' : {'class' : 'form-control'}}) }}</div>
    <div class="form-inline">{{ form_row(form.idUser, {'attr' : {'class' : 'form-control'}}) }}</div>
    <input type="submit" value="Create" />
    {{ form_end(form) }}

    <ul>
        <li>
            <a href="{{ path('offrecovoiturage_index') }}">Back to the list</a>
        </li>
    </ul>
{% endblock %}
